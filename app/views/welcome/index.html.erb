<h1>Задачи</h1>

<% Project.all.each do |project| %>
	<h2><%= project.title %></h2>
	<ul>
		<% project.todos.each do |todo|%>
			<li><%= todo.text %></li>
		<% end %>
	</ul>
<% end %>
	



<%= form_for :article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
